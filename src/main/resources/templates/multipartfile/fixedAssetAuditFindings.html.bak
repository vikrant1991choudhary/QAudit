			function getAuditCompany(){
				$.ajax({
					url : "/getAuditCompany",
					type : "GET",
					async : false,
					contentType : "application/j-son;charset=UTF-8",
					dataType : "json",
					contentType: "json",
					crossDomain : true,
					success : function(data) {
						console.log(data);
						$('#auditCompany').val(data.auditCompany);
						$('#auditYear').val(data.audityear);
						showCompanyLabel();
						getStatus();
					}
				});
			}
			
