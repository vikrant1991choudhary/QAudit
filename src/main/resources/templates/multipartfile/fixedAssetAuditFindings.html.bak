			function getStatus(){
			var cmpnyid = $("#auditCompany>option:selected").val();
				
				var companyName = $("#auditCompany>option:selected").text();
				var finyear = $("#auditYear>option:selected").text();
				var comments = '';
				
			//	alert(cmpnyid);
				 $.ajax({
			        url: "/getFixedAssetAudition?companyid="+cmpnyid,
			        type: "GET",
			        async: false,
			        cache: false,
			        contentType: "application/j-son;charset=UTF-8",
			        dataType: "json",
			        crossDomain: true,
			        success: function (data) {
			        	 $('#cashauditcomments').html(comments);
					     $("#companyfinalabel").html(companyName +" | "+finyear);
						$('#companyhead').css("display","block");
			            $('#fixedassetauditfindings').DataTable().clear().destroy();
			            $('#fixedassetauditfindings').DataTable({

			                data: data,
			                columns: [
			                    { data: "particulars" },
			                    { data: "debit" },
			                    { data: "credit" },
			                   /*  { data: "financialyear" }, */
			                    { data: "openingBalance" },
			                    { data: "closingBalance" },
			                    {
				                    data: "particulars",
				                    mRender: function (
				                        data, type,
				                        full) {
				                        return "<button style=\"width: 55px;\" class=\"btn\"><i class=\"fa fa-paperclip\" aria-hidden=\"true\" "
				                        + " onclick=\"openAddSupportingModal('"
				                         + data
				                         + "')\"></i></button>";
				                    }
				                }
			                    
			                ],
			                "columnDefs" : [ {
								"targets" : 1,//column index
								"data" : "debit",
								"render" : function(data, type,
										row) {
									  data = (Math
											.round(data * 100) / 100)
											.toFixed(2); 
									 
											 data=data.toString();
											    var afterPoint = '';
											    if(data.indexOf('.') > 0)
											       afterPoint = data.substring(data.indexOf('.'),data.length);
											    data = Math.floor(data);
											    data=data.toString();
											    var lastThree = data.substring(data.length-3);
											    var otherNumbers = data.substring(0,data.length-3);
											    if(otherNumbers != '')
											        lastThree = ',' + lastThree;
											    var res = otherNumbers.replace(/\B(?=(\d{2})+(?!\d))/g, ",") + lastThree + afterPoint;
											    console.log(res);
											   
									
									return res;
								}
							} ,{
								"targets" : 2,//column index
								"data" : "credit",
								"render" : function(data, type,
										row) {
									  data = (Math
											.round(data * 100) / 100)
											.toFixed(2); 
									 
											 data=data.toString();
											    var afterPoint = '';
											    if(data.indexOf('.') > 0)
											       afterPoint = data.substring(data.indexOf('.'),data.length);
											    data = Math.floor(data);
											    data=data.toString();
											    var lastThree = data.substring(data.length-3);
											    var otherNumbers = data.substring(0,data.length-3);
											    if(otherNumbers != '')
											        lastThree = ',' + lastThree;
											    var res = otherNumbers.replace(/\B(?=(\d{2})+(?!\d))/g, ",") + lastThree + afterPoint;
											    console.log(res);
											   
									
									return res;
								}
							} ,{
								"targets" : 3,//column index
								"data" : "openingBalance",
								"render" : function(data, type,
										row) {
									  data = (Math
											.round(data * 100) / 100)
											.toFixed(2); 
									 
											 data=data.toString();
											    var afterPoint = '';
											    if(data.indexOf('.') > 0)
											       afterPoint = data.substring(data.indexOf('.'),data.length);
											    data = Math.floor(data);
											    data=data.toString();
											    var lastThree = data.substring(data.length-3);
											    var otherNumbers = data.substring(0,data.length-3);
											    if(otherNumbers != '')
											        lastThree = ',' + lastThree;
											    var res = otherNumbers.replace(/\B(?=(\d{2})+(?!\d))/g, ",") + lastThree + afterPoint;
											    console.log(res);
											   
									
									return res;
								}
							} ,{
								"targets" : 4,//column index
								"data" : "closingBalance",
								"render" : function(data, type,
										row) {
									  data = (Math
											.round(data * 100) / 100)
											.toFixed(2); 
									 
											 data=data.toString();
											    var afterPoint = '';
											    if(data.indexOf('.') > 0)
											       afterPoint = data.substring(data.indexOf('.'),data.length);
											    data = Math.floor(data);
											    data=data.toString();
											    var lastThree = data.substring(data.length-3);
											    var otherNumbers = data.substring(0,data.length-3);
											    if(otherNumbers != '')
											        lastThree = ',' + lastThree;
											    var res = otherNumbers.replace(/\B(?=(\d{2})+(?!\d))/g, ",") + lastThree + afterPoint;
											    console.log(res);
											   
									
									return res;
								}
							} ],
			                dom: 'Bfrtip',
			                buttons: [
			                    'copy', 'csv', 'excel', 'pdf', 'print'
			                ]
			            });
			        }
			    });
				
			}

			function downloadfile(file){

	//window.open("http://localhost:9093/downloadPDF?fileName="+file, '_blank');
